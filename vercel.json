{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/brand/(.*)",
      "dest": "/brand/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(favicon\\.ico|manifest\\.json|robots\\.txt)",
      "dest": "/$1"
    },
    {
      "src": "/(?!api)(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
